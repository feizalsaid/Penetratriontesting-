[?2004h]0;bandit22@bandit: ~[01;32mbandit22@bandit[00m:[01;34m~[00m$ ls
[?2004l[?2004h]0;bandit22@bandit: ~[01;32mbandit22@bandit[00m:[01;34m~[00m$ cd /etc/cron.d
[?2004l[?2004h]0;bandit22@bandit: /etc/cron.d[01;32mbandit22@bandit[00m:[01;34m/etc/cron.d[00m$ cat cronjob_bandit23
[?2004l@reboot bandit23 /usr/bin/cronjob_bandit23.sh  &> /dev/null
* * * * * bandit23 /usr/bin/cronjob_bandit23.sh  &> /dev/null
[?2004h]0;bandit22@bandit: /etc/cron.d[01;32mbandit22@bandit[00m:[01;34m/etc/cron.d[00m$ cat [7m/usr/bin/cronjob_bandit23.sh[27m/usr/bin/cronjob_bandit23.sh
[?2004l#!/bin/bash

myname=$(whoami)
mytarget=$(echo I am user $myname | md5sum | cut -d ' ' -f 1)

echo "Copying passwordfile /etc/bandit_pass/$myname to /tmp/$mytarget"

cat /etc/bandit_pass/$myname > /tmp/$mytarget
